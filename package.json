{
  "name": "@hulla/api",
  "namespace": "@hulla",
  "version": "0.0.0",
  "authors": "Samuel Hulla",
  "maintainers": [
    "Samuel Hulla <hulla@hulla.dev>"
  ],
  "description": "API manager of the future",
  "homepage": "https://hulla.dev/projects/api",
  "repository": {
    "type": "git",
    "url": "https://github.com/hulladev/api.git"
  },
  "type": "module",
  "license": "MIT",
  "scripts": {
    "bundle": "bunchee --dts -m --cwd",
    "build": "bun bundle core",
    "lint": "eslint --ext .ts --fix .",
    "prettier": "prettier --write .",
    "test": "bun test",
    "prepare": "husky",
    "commit": "bunx cz",
    "package": "bunx changeset",
    "release": "bunx changeset publish"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@types/bun": "^1.0.11",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "bunchee": "^5.1.2",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "cz-emoji": "^1.3.2-canary.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "expect-type": "^0.19.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "msw": "^2.2.14",
    "prettier": "^3.2.5",
    "prettier-plugin-organize-imports": "^3.2.4",
    "typescript": "^5.0.0"
  },
  "main": "core/dist/index.js",
  "module": "core/dist/index.esm.js",
  "types": "core/dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./core/dist/index.mjs",
      "module": "./core/dist/index.esm.js",
      "require": "./core/dist/index.js"
    }
  },
  "files": [
    "core/dist"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-emoji"
    },
    "cz-emoji": {
      "skipScope": true,
      "scopes": [],
      "types": [
        {
          "emoji": "‚úÖ",
          "code": ":white_check_mark: feat:",
          "description": "a new functionality",
          "name": "feat"
        },
        {
          "emoji": "üêû",
          "code": ":lady_beetle: fix:",
          "description": "a bug fix",
          "name": "fix"
        },
        {
          "emoji": "üîß",
          "code": ":wrench: update:",
          "description": "a code change that neither fixes a bug nor adds a feature",
          "name": "update"
        },
        {
          "emoji": "üìö",
          "code": ":books: docs:",
          "description": "documentations",
          "name": "docs"
        },
        {
          "emoji": "üß™",
          "code": ":test_tube: tests:",
          "description": "tests",
          "name": "tests"
        },
        {
          "emoji": "ü™õ",
          "code": ":screwdriver: config:",
          "description": "configuration files",
          "name": "config"
        },
        {
          "emoji": "üì¶",
          "code": ":package: release:",
          "description": "new release bundle",
          "name": "release"
        },
        {
          "emoji": "ü§ñ",
          "code": ":robot: devops:",
          "description": "ci/cd or other form of automation",
          "name": "devops"
        }
      ]
    }
  },
  "keywords": [
    "hulla",
    "api",
    "query",
    "query manager",
    "rest",
    "graphql",
    "rpc",
    "http",
    "server actions",
    "@hulla/api",
    "hulla api",
    "@hulla",
    "hulla dev",
    "trpc",
    "axios",
    "swr",
    "firebase",
    "supabase",
    "react-query",
    "@tanstack/react-query",
    "tanstack"
  ]
}
